{
  "$id": "https://squadup-gaming.com/profile-update-request.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Profile Update Request",
  "type": "object",
  "properties": {
    "gamerTag": {
      "type": "string"
    },
    "activisionId": {
      "type": "string"
    },
    "kd": {
      "type": "number"
    },
    "wins": {
      "type": "integer"
    },
    "playStyle": {
      "$ref": "play-style.json"
    },
    "userId": {
      "type": "integer"
    }
  },
  "required": [ "image", "gamerTag", "activisionId", "kd", "wins", "playStyle", "userId" ]
}
